# Example environment variables for Docker Compose
# Copy this file to .env and fill in your values

# Oracle Database Connection
ORACLE_USER=monitor_user
ORACLE_PASSWORD=your_secure_password_here
ORACLE_DSN=oracle-host:1521/service_name

# Application Secrets
SECRET_KEY=change-this-to-a-random-secret-key
JWT_SECRET=change-this-to-another-random-secret

# Redis (for production)
REDIS_PASSWORD=your_redis_password_here

# Environment
ENVIRONMENT=development
DEBUG=true
